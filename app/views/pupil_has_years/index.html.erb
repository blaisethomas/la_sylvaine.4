<h1>Listing pupil_has_years</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Payment</th>
      <th>Pupil</th>
      <th>Adjusted price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pupil_has_years.each do |pupil_has_year| %>
      <tr>
        <td><%= pupil_has_year.year %></td>
        <td><%= pupil_has_year.payment %></td>
        <td><%= pupil_has_year.pupil %></td>
        <td><%= pupil_has_year.adjusted_price %></td>
        <td><%= link_to 'Show', pupil_has_year %></td>
        <td><%= link_to 'Edit', edit_pupil_has_year_path(pupil_has_year) %></td>
        <td><%= link_to 'Destroy', pupil_has_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pupil has year', new_pupil_has_year_path %>
